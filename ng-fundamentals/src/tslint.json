{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            false,
            "element",
            "app",
            "kebab-case"
        ],
        "no-inferrable-types": [
            false,
            "ignore-params",
            "ignore-properties"
        ]
    }
}
